// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
  binaryTargets = ["native", "linux-musl"]
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Artist {
  id String @id @default(uuid())
  name String
  grammy Boolean
}

model Album {
  id String @id @default(uuid())
  name String
  artistId: String
  year: Int
}

model Tracks {
  id String @id @default(uuid())
  name String
  artistId: String
  albumId: String
  duration: Int
}

model Users {
  id String @id @default(uuid())
  login String
  password: String
  version: Int
  createdAt DateTime
  updatedAt DateTime
}

model Favorites {
  id String @id @default(uuid())
}

